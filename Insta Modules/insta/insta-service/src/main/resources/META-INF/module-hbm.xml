<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" default-lazy="false">
	<import class="jwc.insta.model.HandleAPIKeys" />
	<import class="jwc.insta.model.InstaOtherDetails" />
	<import class="jwc.insta.model.InstaSiteImages" />
	<class name="jwc.insta.model.impl.HandleAPIKeysImpl" table="JWC_HandleAPIKeys">
		<id access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" name="handleAPIKeyId" type="long">
			<generator class="increment" />
		</id>
		<property access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" name="handleName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" name="pageKey" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" name="apiKey" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" name="instagramWidgetFlag" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
	</class>
	<class name="jwc.insta.model.impl.InstaOtherDetailsImpl" table="JWC_InstaOtherDetails">
		<id access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" name="otherDetid" type="long">
			<generator class="increment" />
		</id>
		<property access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" name="instaHandleName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" name="followUsLink" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
	<class name="jwc.insta.model.impl.InstaSiteImagesImpl" table="JWC_InstaSiteImages">
		<id access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" name="instaSiteImageId" type="long">
			<generator class="increment" />
		</id>
		<property access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" name="pageKey" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" name="mediaUrl" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" name="instaimageUrl" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" name="timestamp" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" name="otherDetid" type="com.liferay.portal.dao.orm.hibernate.LongType" />
	</class>
</hibernate-mapping>